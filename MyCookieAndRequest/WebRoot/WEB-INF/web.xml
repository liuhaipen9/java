<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>myServlet3</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.cn.cookie.MyServlet3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>myServlet4</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.cn.cookie.MyServlet4</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>myServlet3</servlet-name>
    <url-pattern>/servlet/myServlet3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>myServlet4</servlet-name>
    <url-pattern>/servlet/myServlet4</url-pattern>
  </servlet-mapping>

</web-app>